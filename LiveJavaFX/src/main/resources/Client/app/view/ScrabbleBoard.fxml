<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<BorderPane minHeight="800.0" prefHeight="793.0" prefWidth="759.0" stylesheets="@GameTheme2.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.controller.ScrabbleController">
   <top>
      <HBox maxHeight="80.0" minHeight="80.0" minWidth="100.0" prefHeight="80.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </top>
   <bottom>
      <VBox minHeight="118.0" prefHeight="118.0" prefWidth="759.0" BorderPane.alignment="CENTER">
         <children>
            <HBox maxHeight="80.0" minHeight="80.0" prefHeight="80.0" prefWidth="759.0" />
            <HBox alignment="CENTER" minHeight="39.0" prefHeight="39.0" prefWidth="759.0" spacing="10.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#btnStartGame" text="Start Game" />
                  <Button mnemonicParsing="false" text="End Game" />
                  <Button fx:id="btnUndo" disable="true" mnemonicParsing="false" onAction="#btnUndo_Click" text="Undo" />
                  <Button fx:id="btnRedo" disable="true" mnemonicParsing="false" onAction="#btnRedo_Click" text="Redo" />
               </children>
            </HBox>
         </children>
      </VBox>
   </bottom>
   <center>
      <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
